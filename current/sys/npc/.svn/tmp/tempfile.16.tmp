// 函数：初始化等级
void init_level( object me, int level )    // NPC 专用
{
	string name, *attrib;
	int type, *value1, *value2, *value3, count;
        level = range_value(level, 1, 149);
        me->set_level(level);
        level --;        
        me->set_con(level * 3);
        if ( MAIN_D->get_absolute_random_number()==0 )  level = 140;
        
        // 判断是否场景中的怪物和小boss
        name = get_file_name(me);
        if ((name[0..8]=="/npc/npc/"))
        {
        	me->set_walk_speed( Speed[level] );  me->set_attack_speed( Speed2[level] );
        	level ++;
        	sscanf(name[9..<1], "%d", type);
        	if (  type >0 && arrayp(attrib = npc_attrib[type]))
        	{
        		value1 = npc_type[attrib[0]];
        		value2 = npc_add1[attrib[1]];
        		value3 = npc_add2[attrib[2]];
        		if (arrayp(value1) && arrayp(value2) && arrayp(value3) )
        		{
        			count = (10+level)*(level*4+value1[0]*10+value2[0]*10+value3[0]*10)/100*(154+level*10)/700;
        			me->set_max_hp(count);
        			count = (10+level)*(level*4+value1[1]*10+value2[1]*10+value3[1]*10)/100*(154+level*10)/700;
        			me->set_max_mp(count);
        			count = (10+level)*(level/10+value1[2]+value2[2]+value3[2])/10;
        			me->set_ap(count);
        			count = (10+level)*(level/10+value1[3]+value2[3]+value3[3])/10;
        			me->set_dp(count);
        			count = (10+level)*(level/10+value1[4]+value2[4]+value3[4])/10;
        			me->set_cp(count);
        			count = (10+level)*(level/10+value1[5]+value2[5]+value3[5])/10;
        			me->set_pp(count);
        			count = (10+level)*(level/10+value1[6]+value2[6]+value3[6])/10;
        			me->set_sp(count);      			        			        			
        			return ;
        		}
        	}
        }        
	switch( me->get_char_type() )
	{
	              default :
	    case FIGHTER_TYPE : me->set_max_hp( HP[level] );  me->set_max_mp( MP[level] );
	                        me->set_ap( AP[level] );  me->set_dp( DP[level] );  
	                        me->set_cp( CP[level] );  me->set_pp( PP[level] );  
	                        me->set_sp( SP[level] );
	                        break;
	
	  case FIGHTER_TYPE_2 : me->set_max_hp( HP[level] * 5 / 2 );  me->set_max_mp( MP[level] * 2 );
	                        me->set_ap( AP[level] * 18 / 10 );  me->set_dp( DP[level] * 2 );  
	                        me->set_cp( CP[level] * 2 );  me->set_pp( PP[level] * 2 );  
	                        me->set_sp( SP[level] * 2 );
	                        break;
	}	       
        me->set_walk_speed( Speed[level] );  me->set_attack_speed( Speed2[level] );
}

