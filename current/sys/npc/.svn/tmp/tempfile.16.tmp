//师门挑战初始化数值
int init_family1(object me)
{
	int level;
	int type, *value1, *value2, *value3, count;
	mixed *mxTmp,*nTmp;
	string fam_name;
	
	level = me->get_level();
	if ( !level )
		level = 150;
	fam_name = me->get_fam_name();
	mxTmp = mpFamily[fam_name];
	if ( !mxTmp )
		return 0;
	nTmp = mxTmp[level];
	init_level(me, level);
	me->set_char_picid(nTmp[0]);
	me->set_walk_speed(3 );  
	if ( nTmp[3] == "武器" || nTmp[3] == "空手" )
		me->set_attack_speed( 13 );
	else
		me->set_attack_speed( 22 );
	if ( nTmp[3] == "五雷" )
	{
		me->set_caster(1);
		me->set_attack(1);
	}
	else if ( nTmp[3] == "暗器" )
	{
		me->set_caster(1);
		me->set_attack(2);
	}
	switch ( fam_name )
	{
	default:
		break;
	case "桃花源":
		me->add_max_hp(me->get_max_hp()*9);		
		me->add_ap(me->get_ap()*4/5);		
		break;
	case "禁卫军":	
		me->add_max_hp(me->get_max_hp()*18);		
//		me->add_ap(me->get_ap()*4/5);		
		me->add_dp(me->get_dp()*4/5);		
		break;
	case "蜀山":
		me->add_max_hp(me->get_max_hp()*16);		
//		me->add_ap(me->get_ap()*3/5);		
//		me->add_cp(me->get_cp()*6/5);		
		me->add_dp(me->get_dp()*4/5);		
		me->add_pp(me->get_pp()/2);		
		break;
	case "唐门":	
		me->add_max_hp(me->get_max_hp()*14);		
//		me->add_ap(me->get_ap()*4/5);		
		me->add_dp(me->get_dp()*4/5);		
		me->add_pp(me->get_pp()*4/5);		
		break;
	case "茅山":	
		me->add_max_hp(me->get_max_hp()*16);		
//		me->add_ap(me->get_ap()*4/5);		
//		me->add_cp(me->get_cp()*4/5);		
		me->add_dp(me->get_dp()*4/5);		
		me->add_pp(me->get_pp()/2);		
		break;		
	case "昆仑山":	
		me->add_max_hp(me->get_max_hp()*13);		
//		me->add_ap(me->get_ap()*4/5);		
		me->add_dp(me->get_dp()*4/5);		
//		me->add_cp(me->get_cp()*11/10);		
		me->add_pp(me->get_pp()*7/10);		
		break;	
	case "云梦泽":	
		me->add_max_hp(me->get_max_hp()*18);		
//		me->add_ap(me->get_ap()*4/5);		
		me->add_dp(me->get_dp()*4/5);		
