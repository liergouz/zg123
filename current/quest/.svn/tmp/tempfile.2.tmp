int init_level_pet()
{
	int i,size,level;
	string *nTmp;	
	nTmp = keys(mpBeastInfo);
	for(i=0,size=sizeof(nTmp);i<size;i++)
	{
		if ( member_array(nTmp[i],nShenshou) != -1 )
			continue;
		level = mpBeastInfo[nTmp[i]]["level"];	
		if ( !arrayp(mpLevelPet[level]) )
			mpLevelPet[level] = ({});
		mpLevelPet[level] += ({ nTmp[i] });	
	}
}
//获取等级段的宠物集合
string *get_level_pet(int level)
{
	level /= 5;
	level *= 5;
	if ( level > 110 )
		level = 110;
	return 	mpLevelPet[level];
}
//获取宠物造型
int get_pet_picid(string cName) { return mpPetPic[cName];}

void create()
{
	init_level_pet();
}
