int *get_beast_prop(string cKind)
{
	return mpProp[cKind];	
}

//按几率获取低级技能编号
int get_drop_skill1()
{
	int i,size,p=0,ski=74101;
	int rate = random(10000);
	string *nTmp;
	
	nTmp = keys(mpDropSkill1);
	for(i=0,size=sizeof(nTmp);i<size;i++)
	{
		p += mpDropSkill1[nTmp[i]];
		if(rate>=p)	continue;
		ski = mpBeastSkill[nTmp[i]];
		break;
	}
	return ski;
}

//随机获取高级技能编号
int get_drop_skill2()
{
	int ski;
	string *nTmp;
	nTmp = keys(mpDropSkill2);
	ski = mpBeastSkill[nTmp[random(sizeof(nTmp))]];
	if ( !ski )
		ski=74201;
	return ski;
}

int get_pet_color(string cName) { return mpColor[cName]; }
//技能描述
string get_skill_desc(int skill)
{
	return mpSkiDesc[skill];	
}
//初始化精元丹技能 flag: 1 低级 2 高级
int init_item_skill(object item,int flag)
{
	int skill;
	
	if ( !item )
		return 0;
	if ( flag == 2 )
		skill = get_drop_skill2();
	else
		skill = get_drop_skill1();
	item->set("special",sprintf("%d",skill));
	return 1;
}

//捕抓玉兔类的特殊处理
void get_yutu(object player, object rabbit)
{
	int rand,move;
	object item,present;
	string i;
	rand = random(100);  
	i = rabbit->get_name();
        if(rand < mpBeastYutu[i]["rand"])
        {
		if(sizeof_inventory(player, 1, MAX_CARRY) >= MAX_CARRY )
		{
			send_user(player,"%c%s",'!',"你身上的道具栏空间不够！");
			return;
		}
        	//捕捉成功。
        	send_user(player, "%c%s", '!', "你捕捉了" + rabbit->get_name());
        	rabbit->remove_from_scene();//从场景里面移出
        	rabbit->before_destruct();
		destruct(rabbit);
		if( objectp( item = present(mpBeastYutu[i]["name"], player, 1, MAX_CARRY) ) && item->get_amount() < 30 )
		{
			item->add_amount(1);				
		}
		else
		{
			present = new(mpBeastYutu[i]["file"]);
			if(!present)
			{
				return;
			}
			if(move = present->move(player,-1))
			{
				present->add_to_user(move);
			}	
			else
			{
				destruct(present);
				return;
			}
			return ;
		}
        }	
        else
        {
        	send_user(player, "%c%s", '!', "捕抓"+rabbit->get_name()+"失败！");
        	return;
        }
}